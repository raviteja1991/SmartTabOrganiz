{
    "manifest_version": 3,
    "name": "Smart Tab Organizer",
    "version": "1.0",
    "description": "Organize and manage your browser tabs effortlessly.",
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "1": "icons/TabIcon1.png",
      "2": "icons/Tab1.png"
    },
    "permissions": ["storage", "tabs", "activeTab", "tabGroups"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "1": "icons/TabIcon1.png",
        "2": "icons/Tab1.png"
      }
    }
  }
  